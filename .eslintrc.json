{
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends":  ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-restricted-syntax":"off",
      "no-await-in-loop":"off",
      "object-curly-newline": "off",
      "consistent-return": "off",
      "camelcase": "off",
      "no-underscore-dangle": "off",
      "comma-dangle": "off",
      "no-param-reassign": "off",
      "no-bitwise": "off",
      "no-plusplus": "off",
      "max-classes-per-file": "off",
      "default-case": "off",
      "prefer-destructuring": ["error", {
        "object": true,
        "array": false
      }],
      "no-unused-vars": "off",
      "no-mixed-operators": "off",
      "global-require": "off"
    }
  }